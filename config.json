{
  "walk_dir": "/your-project-dir",
  "with_filepath?": false,
  "with_filename?": true,
  "skip_when_character": [
    "#",
    "!",
    "\n",
    "\r",
    "/",
    " /",
    " *"
  ],
  "allow_files_suffix": [
    ".js",
    ".jsx",
    ".rb",
    ".ts",
    ".tsx"
  ],
  "output_filename": "out1.txt"
}